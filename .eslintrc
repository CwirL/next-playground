{
	"settings": {
		"react": {
			"pragma": "React",
			"version": "16.12.0"
		}
	},
	"env": {
		"browser": true,
		"es6": true
	},
	"extends": [
		"eslint:recommended",
		"plugin:react/recommended"
	],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 2020,
		"sourceType": "module"
	},
	"plugins": [
		"react"
	],
	"rules": {
		"comma-dangle": "off",
		"no-unused-vars": "warn",
		"no-console": "warn"
	}
}
